.es-tree-view, .es-tree-view  ul { margin:0; padding:0; list-style:none; }
.es-tree-view ul { margin-left:1em; position:relative }
.es-tree-view ul.level-1 { margin-left: 0em; }
.es-tree-view ul ul { margin-left:.5em }
.es-tree-view ul:before { content:""; display:block; width:0; position:absolute; top:0; bottom:0; left:0; border-left:1px dotted; }
.es-tree-view ul.level-1:before { display:none; }
.es-tree-view li { margin:0; padding:0 1em; line-height:2em; color:#555; position:relative; cursor:pointer; font-family: "Opensans"; font-size: 14px; padding-top: 1px; padding-bottom: 1px; }
.es-tree-view ul li:before { content:""; display:block; width:10px; height:0; border-top:1px dotted; margin-top:-1px; position:absolute; top:1em; left:0 }
.es-tree-view > ul.level-1 > li:before { display:none; }
/** .es-tree-view ul li:last-child:before { background:#293742; height:auto; top:1em; bottom:0 } **/
.es-tree-view ul li img { height:14px; }
.es-tree-view li a { text-decoration: none; color:#369; }
.es-tree-view .level-1 li ul, .es-tree-view .level-1 li > ul > li { display:none; }
.es-tree-view .level-1 li.open ul, .es-tree-view .level-1 li.open > ul > li{ display:block; }

