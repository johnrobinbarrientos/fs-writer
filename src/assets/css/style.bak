/** ===== kenji override css start ===== **/

:root {
  /*color schemes*/
  --primary: #367bf5;
  --success: #2fa84f;

  --es-darkblue: #496d7d;

  --whitesh: #f7f9fa;
  --whiteblueish: #f2f6fa;
  --whiteblue: #cbd6e2;
  
  --blue: #0056b3;

  --thin-white: #f1f3f4;
  --thin-navy: #324451;

  --darkgrey: #618199;
  --lightgrey: #c0d7e9;

  /*major dark mode colors*/
  --navy: #0f1c24;
  --greyblue: #78909c;
  --grey: #bac7cd;
}

/*utilities*/
.mr-auto { margin-right: auto!important; }
p, a, li, span, label, button, div { font-family: "Opensans" !important; font-size: 14px; }

.menubar .menubar-menu-button div { font-size: 13px; }
.es-top-parent{
  position: relative;
  background: var(--whitesh);
  text-align: left;
}


.es-top-nav { position: relative; display: flex; padding: 10px 0; align-items: center; }
.es-top-nav .nav-btn,
.es-top-nav .nav-toggle {
  width: 77px;
  padding: 9px;
  border: 1px solid #fff;
  background: #fff;
  border-radius: 5px;
  display: inline-block;
  text-decoration: none;
  margin: 0 5px;
}
.es-top-nav .nav-toggle { width: 85px; }
.es-top-nav .user-btn {
  width: 50px;
  height: 50px;
  margin-right: 20px;
  border-radius: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  text-decoration: none;
  background: var(--es-darkblue);
}
.es-top-nav .user-btn > i { font-size: 28px; color: #fff; }
.es-top-nav .user-btn > i.fas { font-size: 20px; color: #fff; }

.es-top-nav .nav-btn > span, .nav-toggle .text { line-height: 1; font-size: 12px; font-weight: 600; }
.es-top-nav .nav-btn > i { padding-bottom: 7px; font-size: 20px; }

.es-top-nav .nav-btn > span,
.es-top-nav .nav-btn > i,
.es-top-nav .nav-toggle .text,
.es-top-nav .nav-toggle .switch {
  display: block;
  text-align: center;
}

.nav-btn.new-book, .nav-btn.sync-data { color: var(--primary); transition: 0.4s; -webkit-transition: .4s; }
.nav-btn.new-book:hover, .nav-btn.sync-data:hover { color: var(--blue); transition: 0.4s; -webkit-transition: .4s; }

.nav-btn.dashboard,
.nav-btn.webinars,
.nav-btn.notes { color: var(--success); }

.nav-btn.dashboard span,
.nav-btn.webinars span,
.nav-btn.notes span { color: #333; }

/*special nav-btn*/
.es-top-nav .inner-nav { display: flex; margin: 0 5px; }
.es-top-nav .inner-nav .nav-btn { width: 85px; border-radius: 0; margin: 0; background: var(--thin-white); border-color: var(--thin-white); }
.es-top-nav .inner-nav .nav-btn:first-child { border-top-left-radius: 5px; border-bottom-left-radius: 5px; }
.es-top-nav .inner-nav .nav-btn:last-child { border-top-right-radius: 5px; border-bottom-right-radius: 5px; }
.es-top-nav .inner-nav .nav-btn.active { 
